#include <stdio.h>
int main(){
    int age =  21;
    float price = 19.99;
    double pi = 3.14159265;
    char currency = '$';
    char name[] = "Namat ULlah";
    printf("My age is %d years.\n", age);
    printf("Price of one pizza is %.2f$.\n", price);
    printf("Value of Pi is %f.\n", pi);
    printf("The international currency is %c.\n", currency);
    printf("My name is %s.\n", name);
    
}
