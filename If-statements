#include <stdio.h>
int main(){
    int age = 0;
    printf("Enter your age : ");
    scanf("%d", &age);
    
    if(age >= 65){
        printf("You are senior citizen.");
        }
    
    else if(age >= 18){
    printf("You are eligible for vote.");
        }
    
    else if(age < 0){
        printf("You have not burn yet.");
        }
    
    else if(age == 0){
        printf("You are new burn yet.");
        }
    
    else{
        printf("You are not eligible for vote.");
    }
    
    return 0;
    
}
